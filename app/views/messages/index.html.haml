.layout
  .layout_dashboard
    .dashboard
      .dashboard_header
        .dashboard_name wakugami
        %ul.dashboard_iconWrap
          %li.dashboard_edit
            %a{href: "a"}
              %i.fa.fa-edit
          %li.dashbord_setting
            %a{href: "a"}
              %i.fa.fa-cog

      .dashboard_body
        %ul.dashboard_groupWrap
          %li.dashboard_group
            %a{href: "a"}
              .dashboard_groupName sample1
              .dashboard_groupNewComment Good bye work
          %li.dashboard_group
            %a{href: "a"}
              .dashboard_groupName sample2
              .dashboard_groupNewComment Hello
          %li.dashboard_group
            %a{href: "a"}
              .dashboard_groupName example
              .dashboard_groupNewComment まだメッセージがありません
  .layout_timeline
    .timeline
      .timeline_header
        .timeline_groupWrap
          .timeline_groupName Sample
          .timeline_membersName Member：wakugami
        %a.timeline_editBtn Edit
      .timeline_body
        %ul
          %li.timeline_bodyList
            .timeline_bodyInfoWrap
              .timeline_bodyName wakugami
              .timeline_bodyDate 2019/04/04(Thu) 14:41:27
